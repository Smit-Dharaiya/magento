<?xml version='1.0'?>
<layout>
    <user_adminhtml_user_index>
        <reference name="content">
            <block type="user/adminhtml_user" name="user_grid"></block>
        </reference>
    </user_adminhtml_user_index>
    <user_adminhtml_user_edit>
        <reference name="content">
            <block type="user/adminhtml_user_edit" name="user_edit" />
        </reference>
        <reference name="left">
            <block type="user/adminhtml_user_edit_tabs" name="user_tabs" />
        </reference>
        <reference name="js">
            <block type="adminhtml/catalog_product_edit_js" template="catalog/product/js.phtml" name="catalog_product_js"></block>
            <block type="core/template" template="catalog/wysiwyg/js.phtml" />
        </reference>
    </user_adminhtml_user_edit>
    <user_adminhtml_user_attribute_index>
        <reference name="content">
            <block type="user/adminhtml_user_attribute" name="user_attribute_grid"></block>
        </reference>
    </user_adminhtml_user_attribute_index>
    <user_adminhtml_user_attribute_edit>
        <reference name="left">
            <block type="user/adminhtml_user_attribute_edit_tabs"></block>
        </reference>
        <reference name="content">
            <block type="user/adminhtml_user_attribute_edit" name="user_attribute_edit"></block>
        </reference>
        <reference name="js">
            <block type="adminhtml/template" name="attribute_edit_js" template="user/attribute/js.phtml"></block>
        </reference>
    </user_adminhtml_user_attribute_edit>
    <user_adminhtml_user_set_index>
        <reference name="js">
            <block type="adminhtml/template" name="attribute_edit_set_js" template="user/attribute/set/main.phtml"></block>
        </reference>
    </user_adminhtml_user_set_index>
</layout>
